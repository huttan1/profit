	<!-- Log4Net standard configuration, same behavior as prior 4.4 SP2 -->
	<!-- For custom settings see, http://logging.apache.org/log4net/release/manual/configuration.html -->
	<log4net>
		<!-- Litium Studio Log Appender, writes logs to database. Logs in same pattern as Solution.Instance.Log.CreatLogEntry(...) did prior to 4.4 SP2 -->
		<appender name="LitiumStudioDBLog" type="Litium.Foundation.Log.Log4NetAppender"/>
		<!-- Sample file logger, only logs debug through filter-->
		<appender name="LitiumStudioFileLog" type="log4net.Appender.RollingFileAppender">
			<file value="..\litiumstudio.log"/>
			<appendToFile value="true"/>
			<rollingStyle value="Size"/>
			<maxSizeRollBackups value="10"/>
			<maximumFileSize value="10MB"/>
			<staticLogFileName value="true"/>
			<layout type="log4net.Layout.PatternLayout">
				<conversionPattern value="%date [%thread] %-5level %logger [%property{NDC}] - %message%newline"/>
			</layout>
		</appender>
		<!-- Root level, all loggers write according to these settings if not explicitly definend in other logger.  -->
		<root>
			<level value="INFO"/>
			<!-- Default startup loglevel, can be changed in runtime but is not persistent. DEBUG, INFO, WARN, ERROR, FATAL -->
			<!-- Standard logger, stores in database. -->
			<appender-ref ref="LitiumStudioDBLog"/>
			<!-- Sample file logger, uncomment if you want logging to disc -->
			<!--<appender-ref ref="LitiumStudioFileLog" />-->
		</root>
	</log4net>
	<!-- Specify to build with compiler for .Net 3.5 -->
